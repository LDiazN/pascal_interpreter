program Arithmetc;

var
    a, b, addResult, subResult, multResult, divResult : real;

begin
    a := 5.0;
    b := 5.0;
    addResult := a + b;
    subResult := a - b;
    multResult := a * b;
    divResult := a / b;
    writeln(addResult);
    writeln(subResult);
    writeln(multResult);
    writeln(divResult);
end.